<% @recipes.each do |recipe| %>
  <div class="row">
    <div class="col s12 m7">
      <div class="card">
        <div class="card-image">
          <img src="<%=recipe["image"]%>">
          <span class="card-title"></span>
        </div>
        <div class="card-content">
        </div>
        <div class="card-action">
          <a href="#"><%=recipe["name"]%></a>
        </div>
      </div>
    </div>
  </div>
  <% end %>
